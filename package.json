{
  "name": "es6-react-component",
  "version": "7.0.6",
  "description": "Card component with i18n and bundled styling targeting Bootstrap 3 - powered by React.",
  "main": "./demo/dist.card-component.js",
  "scripts": {
    "start": "npm run dev",
    "dev": "NODE_ENV=development webpack-dev-server --hot",
    "pretest": "npm run lint",
    "test": "jest",
    "test-cov": "jest --coverage",
    "build": "webpack -p",
    "coveralls": "coveralls < coverage/lcov.info",
    "lint": "eslint ./react_components/**",
    "lint-all": "eslint react_components/** __tests__/** demo/demo-scriptinclude.js",
    "ci": "npm test && npm run build",
    "release": "node ./npm_scripts/release.js"
  },
  "jest": {
    "transform": {
      ".*": "./node_modules/babel-jest"
    },
    "unmockedModulePathPatterns": [
      "react",
      "react-dom",
      "enzyme"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/setup.test.js"
  },
  "author": "Aaron Kaka <aaron.kaka@gmail.com>",
  "contributors": [
    {
      "name": "Deepak Bhagchandani",
      "email": "deepak2510@gmail.com"
    }
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/aaronkaka/es6-react-component"
  },
  "dependencies": {
    "prop-types": "^15.6.0",
    "react-intl": "^2.3.0"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-react-intl": "^2.3.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "coveralls": "~2.11.9",
    "css-loader": "~0.23.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "~1.10.3",
    "eslint-loader": "~1.1.1",
    "eslint-plugin-react": "~3.16.1",
    "file-loader": "~0.8.5",
    "jest": "^20.0.4",
    "raf": "^3.4.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-test-renderer": "^16.2.0",
    "semver": "~5.1.0",
    "style-loader": "~0.13.0",
    "url-loader": "~0.5.7",
    "webpack": "^3.4.1",
    "webpack-dev-server": "^2.5.1"
  },
  "keywords": [
    "card",
    "react-component",
    "es2015"
  ]
}
